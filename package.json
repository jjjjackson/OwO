{
  "name": "owo",
  "version": "1.4.1",
  "description": "OpenCode plugins with agents, background tasks for parallel execution, and smart orchestration",
  "keywords": [
    "agents",
    "ai",
    "background-tasks",
    "llm",
    "opencode",
    "oracle",
    "orchestration",
    "owo",
    "parallel",
    "plugin"
  ],
  "license": "MIT",
  "author": "rawtoast",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RawToast/owo.git"
  },
  "bin": {
    "owo": "./dist/cli/index.js"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "catalog": {
      "zod": "3.24.1",
      "strip-json-comments": "5.0.3",
      "@opencode-ai/plugin": "1.1.25",
      "@opencode-ai/sdk": "1.1.25"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm && bun build src/cli/index.ts --outdir dist/cli --target node --format esm && tsgo --emitDeclarationOnly",
    "prepublishOnly": "bun run clean && bun run build",
    "lint": "oxlint",
    "format": "oxfmt",
    "dev": "bun run build && opencode",
    "make": "turbo run make",
    "make:local": "turbo run make:local  --filter='./packages/*'",
    "make:packages": "turbo run make --filter='./packages/*'",
    "compile": "turbo run compile",
    "clean:packages": "turbo run clean --filter='./packages/*'"
  },
  "dependencies": {
    "@clack/prompts": "0.11.0",
    "@opencode-ai/plugin": "1.1.25",
    "@opencode-ai/sdk": "1.1.25",
    "@typescript/native-preview": "7.0.0-dev.20260120.1",
    "commander": "14.0.2",
    "picocolors": "1.1.1",
    "strip-json-comments": "5.0.3",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "bun-types": "1.3.6",
    "oxfmt": "0.24.0",
    "oxlint": "1.39.0",
    "turbo": "2.5.4"
  },
  "packageManager": "bun@1.3.6"
}
